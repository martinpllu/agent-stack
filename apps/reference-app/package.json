{
  "name": "reference-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev > .sst/log/web.log 2>&1",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "pnpm test run && pnpm test:e2e",
    "test:quick": "pnpm test run && pnpm test:e2e tests/auth.e2e.test.ts",
    "db": "tsx scripts/db.ts",
    "migrate": "tsx scripts/migrate.ts",
    "sql": "tsx scripts/sql-query.ts",
    "create-user": "tsx scripts/create-user.ts",
    "delete-all-data": "tsx scripts/delete-all-data.ts",
    "dotagent": "dotagent export --formats claude,cursor --no-gitignore"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.844.0",
    "@aws-sdk/client-rds-data": "^3.839.0",
    "@openauthjs/openauth": "^0.4.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "dotagent": "^2.7.0",
    "drizzle-orm": "^0.44.2",
    "hono": "^4.7.4",
    "isbot": "^5.1.27",
    "lucide-react": "^0.456.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "sst": "3.17.8",
    "tailwind-merge": "^2.6.0",
    "valibot": "^1.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@react-router/dev": "^7.5.3",
    "@remix-run/eslint-config": "^2.14.0",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.7.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.19.0",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}
